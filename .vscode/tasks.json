{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "build",
			"command": "dotnet",
			"type": "process",
			"args": [
				"build",
				"${workspaceFolder}/demo/DotFeather.Demo.csproj"
			],
			"problemMatcher": "$tsc"
		},
		{
			"label": "publish",
			"command": "dotnet",
			"type": "process",
			"args": [
				"publish",
				"${workspaceFolder}/demo/DotFeather.Demo.csproj"
			],
			"problemMatcher": "$tsc"
		},
		{
			"label": "watch",
			"command": "dotnet",
			"type": "process",
			"args": [
				"watch",
				"run",
				"${workspaceFolder}/demo/DotFeather.Demo.csproj"
			],
			"problemMatcher": "$tsc"
		}
	]
}
